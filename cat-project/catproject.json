{
    "name": "jqueryspa-test",

    "source": "src/",
    "target": "target/",
    "cattarget": "./",

    "host": "localhost",
    "port": "8080",
    "protocol": "http",
    "analytics": "Y",
    "apppath": "./../web",
    "appserver": {
        "host": "localhost",
        "port": "8080",
        "protocol": "http"
    },

    "runner": {
        "run": {
            "devices": [
                {
                    "disable": false,
                    "type": "localpc",
                    "runner": {
                        "name": "chrome",
                        "address": "/index.html",
                        "options": {
                            "instances": 1
                        }
                    }
                },
                {
                    "disable": false,
                    "type": "localpc",
                    "runner": {
                        "name": "firefox",
                        "address": "/index.html",
                        "options": {
                            "instances": 2
                        }
                    }
                },
                {
                    "disable": false,
                    "type": "android",
                    "id": "all",
                    "runner": {
                        "name": "apk",
                        "options": {
                            "path": "./lib/resources"
                        }
                    }
                }               
            ]
        },
        "server": {
            "host": "192.168.137.74",
            "port": "8080"
        }
    },

    "plugins": [

    ],

    "tasks": [

    ]


}
